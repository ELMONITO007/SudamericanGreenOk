@model Entities.Marca

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}




<link href="~/Content/modern-business.css" rel="stylesheet" />

<div>
    <h3>¿Esta seguro de eliminar el "" <i>"@Html.DisplayFor(model => model.marca)"</i>?</h3>
    <hr />


    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
         @Html.HiddenFor(model => model.Id)
<div class="form-actions no-color">
    <input type="submit" value="Eliminar" style="margin-left:1%" class="btn btn-outline-info" />
    <br/>
    <input type="button" class="btn btn-outline-primary " style="float:left;margin-left:1%;margin-top:3%" value="Volver" onclick="location.href='@Url.Action("Index", "Marca")'" />
</div>
    }
</div>







<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
